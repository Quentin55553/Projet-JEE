<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="org.jee.Data.Cours">
        <table name="cours" schema="gestion_scolarite" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id_cours"/>
            </id>
            <basic name="nom">
                <column name="nom_cours"/>
            </basic>
            <basic name="description">
                <column name="description"/>
            </basic>
            <basic name="idProfesseur">
                <column name="id_enseignant"/>
            </basic>
            <many-to-one name="personneByIdEnseignant" optional="true" target-entity="org.jee.Data.Personne">
                <join-column name="id" nullable="false" referenced-column-name="id_personne"/>
            </many-to-one>
            <one-to-many name="inscriptionsByIdCours" mapped-by="coursByIdCours" target-entity="org.jee.Data.Inscription"/>
            <one-to-many name="resultatsByIdCours" mapped-by="coursByIdCours" target-entity="org.jee.Data.Resultat"/>
        </attributes>
    </entity>
</entity-mappings>
