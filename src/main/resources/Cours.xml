<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.jee.entity.Cours" table="Cours" schema="gestion_scolarite">
        <id name="idCours" column="id_cours"/>
        <property name="nomCours" column="nom_cours"/>
        <property name="description" column="description"/>
        <property name="dateDebut" column="date_debut"/>
        <property name="dateFin" column="date_fin"/>
        <property name="idEnseignant" column="id_enseignant"/>
    </class>
</hibernate-mapping>