<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.jee.entity.Inscription" table="Inscription" schema="gestion_scolarite">
        <id name="idInscription" column="id_inscription"/>
        <property name="dateInscription" column="date_inscription"/>
        <property name="etat" column="etat"/>
        <property name="descriptionRefus" column="description_refus"/>
        <property name="idEtudiant" column="id_etudiant"/>
    </class>
</hibernate-mapping>